generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id       Int      @id @default(autoincrement())
  phone    Int?     @unique
  email    String?  @unique
  name     String
  avatar   String?
  createAt DateTime @default(now())
  updateAt DateTime @updatedAt
}

model Dayoff {
  id       Int      @id @default(autoincrement())
  createAt DateTime @default(now())
  updateAt DateTime @updatedAt
}
